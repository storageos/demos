[global]
size=1GB
runtime=900
time_based=1
ioengine=libaio
direct=1
random_generator=tausworthe
random_distribution=random
rw=randrw
rwmixread=60
rwmixwrite=40
percentage_random=85
bs=4k
iodepth=64
log_avg_msec=250
group_reporting=1

[vol0]
filename=/mnt/pvc0/fio.dat

[vol1]
filename=/mnt/pvc1/fio.dat

[vol2]
filename=/mnt/pvc2/fio.dat

[vol3]
filename=/mnt/pvc3/fio.dat
